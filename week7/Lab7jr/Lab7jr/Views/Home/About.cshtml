@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>


<h4>My favorite movies list</h4>
<ul>

    @{


        for (var i = 0; i < ViewBag.Movies.Length; i++)
        {
        <li>@ViewBag.Movies[i]</li>
        }


    }
</ul>
<h4>My favorite colors list</h4>
<ul>

    @{
        foreach (var myFavoriteColor in ViewBag.MyFavoriteColor)
        {
            <li>@myFavoriteColor</li>
            }
        }

</ul>